package programmers;

import java.util.Arrays;

public class Quiz120821 {
	public static void main(String[] args) {
		
		int[] arr = {20,70,40,50,60};
		

		
		Arrays.sort(arr);
		int max = arr[arr.length - 1];
		int max2 = arr[arr.length -2];
		
		int answer = max * max2;
		
		System.out.println(answer);
		
		
		
		
		

	
	
		
		

	}

}
