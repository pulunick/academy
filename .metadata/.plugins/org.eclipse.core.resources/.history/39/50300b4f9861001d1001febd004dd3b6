package programmers;



public class Quiz120821 {
	public static void main(String[] args) {
		int money = 5500;
		int aa = 5500;
		
		int[] answer = new int[2];
		
		for(int i = 0; i < answer.length; i++) {
			if(i==0) {
				answer[0] = money / aa;
			}
			else {answer[1] = money % aa;}
			
			System.out.println(answer[1]);
			System.out.println(answer[0]);
		}

	}

}
