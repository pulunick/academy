package programmers;

import java.util.Arrays;

public class Quiz120821 {
	public static void main(String[] args) {
		
		int[] arr = {1,2,3,4,5};
		
		
		int max = arr[0];
		int max2 = arr[0];
		

		
		Arrays.sort(arr);
		max = arr[arr.length - 1];
		max2 = arr[arr.length -2];
		
		int answer = max * max2;
		
		System.out.println(answer);
		
		
		
		
		

	
	
		
		

	}

}
